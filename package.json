{
  "name": "urlaub-im-muellerhaus",
  "description": "Website for www.urlaub-im-muellerhaus.de",
  "version": "2.0.0",
  "author": "Ute Lochner, Markus Eckstein",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run extract-iptc && astro dev --host",
    "build": "npm run extract-iptc && astro build",
    "preview": "astro preview",
    "clean": "rm -rf dist .astro",
    "extract-iptc": "node scripts/extract-iptc.mjs",
    "format": "prettier --write \"**/*.{astro,js,ts,json,md,css}\""
  },
  "dependencies": {
    "astro": "^5.17.2",
    "glightbox": "3.3.0",
    "sharp": "0.33.0"
  },
  "devDependencies": {
    "node-iptc": "1.0.5",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markuseckstein/urlaub-im-muellerhaus.de"
  }
}
